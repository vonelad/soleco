{
  "name": "@soleco/lucide",
  "version": "0.0.1",
  "description": "Lucide icons as Solid components",
  "private": false,
  "keywords": [
    "solid",
    "icons",
    "icon",
    "lucide",
    "svg",
    "feather",
    "fontawesome"
  ],
  "homepage": "https://github.com/vonelad/soleco/tree/main/packages/lucide",
  "bugs": "https://github.com/vonelad/soleco/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vonelad/soleco.git",
    "directory": "packages/lucide"
  },
  "license": "MIT",
  "author": "vonelad",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "main": "dist/lucide.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "compile": "tsx ./compile.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "lucide-static": "^0.298.0",
    "solid-js": "^1.8.7",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "solid-js": "^1.8.7"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "format": [
      "esm"
    ],
    "bundle": false,
    "entryPoints": [
      "src/index.ts",
      "src/*.tsx"
    ]
  }
}
